{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "generate-content.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/generate-content",
      "dest": "/generate-content.js",
      "methods": ["POST"]
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}


